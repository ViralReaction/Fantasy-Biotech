<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <LayoutRoomDef>
    <defName>VR_AncientRechargeRoom</defName>
    <canMergeWithAdjacentRoom>true</canMergeWithAdjacentRoom>
    <prefabs>
      <VR_AncientBasicRecharger_Edge>
        <countPerTenEdgeCells>2</countPerTenEdgeCells>
        <minMaxRange>5~15</minMaxRange>
      </VR_AncientBasicRecharger_Edge>
      <VR_AncientStandardRecharger>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~1</minMaxRange>
      </VR_AncientStandardRecharger>
      <VR_AncientLargeMechGestator>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~1</minMaxRange>
      </VR_AncientLargeMechGestator>
      <VR_AncientLamp_Steam>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>0~1</minMaxRange>
      </VR_AncientLamp_Steam>
    </prefabs>
    <floorTypes>
      <li>VR_AncientTile</li>
    </floorTypes>
    <scatter>
      <VR_ChunkConstructSlag>
        <groupsPerHundredCells>2~4</groupsPerHundredCells>
        <itemsPerGroup>1~2</itemsPerGroup>
        <groupDistRange>3~5</groupDistRange>
        <minGroups>1</minGroups>
      </VR_ChunkConstructSlag>
    </scatter>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>VR_AncientMechGestatorRoom</defName>
    <canMergeWithAdjacentRoom>true</canMergeWithAdjacentRoom>
    <prefabs>
      <VR_AncientMechGestator_Cluster>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~5</minMaxRange>
      </VR_AncientMechGestator_Cluster>
      <VR_AncientLargeMechGestator>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~5</minMaxRange>
      </VR_AncientLargeMechGestator>
      <VR_AncientLamp_Steam>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>0~1</minMaxRange>
      </VR_AncientLamp_Steam>
    </prefabs>
    <floorTypes>
      <li>VR_AncientTile</li>
    </floorTypes>
    <scatter>
      <VR_ChunkConstructSlag>
        <groupsPerHundredCells>2~4</groupsPerHundredCells>
        <itemsPerGroup>1~2</itemsPerGroup>
        <groupDistRange>3~5</groupDistRange>
        <minGroups>1</minGroups>
      </VR_ChunkConstructSlag>
    </scatter>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>VR_AncientMechGeneratorRoom</defName>
    <prefabs>
      <VR_AncientGenerator_SmallCluster>
        <countPerHundredCells>2</countPerHundredCells>
        <minMaxRange>1~2</minMaxRange>
      </VR_AncientGenerator_SmallCluster>
      <VR_AncientBarrel_Wooden_Cluster>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~5</minMaxRange>
      </VR_AncientBarrel_Wooden_Cluster>
      <VR_AncientLamp_Steam>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>0~1</minMaxRange>
      </VR_AncientLamp_Steam>
    </prefabs>
    <floorTypes>
      <li>VR_AncientTile</li>
    </floorTypes>
    <scatter>
      <Filth_OilSmear>
        <groupsPerHundredCells>2~4</groupsPerHundredCells>
        <itemsPerGroup>1~2</itemsPerGroup>
        <groupDistRange>3~5</groupDistRange>
        <minGroups>1</minGroups>
      </Filth_OilSmear>
    </scatter>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>VR_AncientBandNodeRoom</defName>
    <minSingleRectWidth>10</minSingleRectWidth>
    <minSingleRectHeight>10</minSingleRectHeight>
    <prefabs>
      <VR_AncientBandNode_Cluster>
        <countPerHundredCells>10</countPerHundredCells>
        <minMaxRange>5~15</minMaxRange>
      </VR_AncientBandNode_Cluster>
    </prefabs>
    <floorTypes>
      <li>VR_AncientTile</li>
    </floorTypes>
    <edgeTerrain>VR_AncientTile</edgeTerrain>
    <scatter>
      <VR_ChunkConstructSlag>
        <groupsPerHundredCells>2~4</groupsPerHundredCells>
        <itemsPerGroup>1~2</itemsPerGroup>
        <groupDistRange>3~5</groupDistRange>
        <minGroups>1</minGroups>
      </VR_ChunkConstructSlag>
      <VR_AncientPipe_Steam>
        <groupsPerHundredCells>2~4</groupsPerHundredCells>
        <itemsPerGroup>8~10</itemsPerGroup>
        <groupDistRange>3~5</groupDistRange>
        <minGroups>1</minGroups>
      </VR_AncientPipe_Steam>
    </scatter>
  </LayoutRoomDef>
  
  <LayoutRoomDef>
    <defName>VR_ComplexCorridor</defName>
    <roomContentsWorkerType>FantasyBiotech.RoomContents_ComplexCorridor</roomContentsWorkerType>
	<floorTypes>
      <li>VR_AncientTile</li>
    </floorTypes>
	<modExtensions>
		<li Class="FantasyBiotech.RoomContentExtension" MayRequire="dankpyon.medieval.overhaul">
			<shelfThing>DankPyon_RuinedShelfEmpty</shelfThing>
			<shelfStuff>WoodLog</shelfStuff>
		</li>
	</modExtensions>
  </LayoutRoomDef>


</Defs>