    <ThingDef ParentName="BuildingBase">
        <defName>VR_AncientSteamGenerator</defName>
        <label>ancient steam generator</label>
        <description>An ancient steam generator. Its usable parts were smashed or looted long ago, and the rest has rusted into uselessness.</description>
        <rotatable>false</rotatable>
        <noRightClickDraftAttack>true</noRightClickDraftAttack>
        <graphicData>
            <texPath>Things/Building/Ruins/SteamGenerator_Ruined</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(2,2)</drawSize>
            <shadowData>
                <volume>(1.9, 0.85, 1.75)</volume>
            </shadowData>
        </graphicData>
        <size>(2,2)</size>
        <fillPercent>0.5</fillPercent>
        <altitudeLayer>Building</altitudeLayer>
        <statBases>
            <MaxHitPoints>250</MaxHitPoints>
            <WorkToBuild>200</WorkToBuild>
            <Flammability>1.0</Flammability>
        </statBases>
        <passability>PassThroughOnly</passability>
        <pathCost>50</pathCost>
        <building>
            <canLandGravshipOn>true</canLandGravshipOn>
            <isInert>true</isInert>
        </building>
    </ThingDef>

