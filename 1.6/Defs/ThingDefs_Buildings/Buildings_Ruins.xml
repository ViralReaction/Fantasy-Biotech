    <ThingDef ParentName="BuildingBase">
        <defName>VR_AncientLamp_Steam</defName>
        <label>ancient steam lamp</label>
        <description>An ancient steam lamp. Valuable parts are missing, and everything else is degraded to uselessness.</description>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <selectable>true</selectable>
        <fillPercent>0.20</fillPercent>
        <pathCost>14</pathCost>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
        <rotatable>false</rotatable>
        <noRightClickDraftAttack>true</noRightClickDraftAttack>
        <graphicData>
            <graphicClass>Graphic_Multi</graphicClass>
            <texPath>Things/Building/Furniture/WallLight_Steam/WallLight_Steam</texPath>
            <drawSize>(2,2)</drawSize>
        </graphicData>
        <comps>
            <li Class="CompProperties_Glower">
                <glowRadius>5.5</glowRadius>
                <glowColor>(71,43,6,0)</glowColor>
            </li>
        </comps>
        <statBases>
            <MaxHitPoints>50</MaxHitPoints>
            <Flammability>0.5</Flammability>
        </statBases>
        <building>
            <canLandGravshipOn>true</canLandGravshipOn>
            <isInert>true</isInert>
        </building>
    </ThingDef>

